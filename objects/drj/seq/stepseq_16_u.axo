<objdefs appVersion="1.0.9">
  <obj.normal id="stepseq_4_u" uuid="7aa423ea71e103abc56210c14bff9c17972947ac">
    <sDescription>Step sequencer, unipolar value x 4</sDescription>
    <author>Are Leistad</author>
    <license>BSD</license>
    <inlets>
      <int32 name="step" description="Step select input"/>
    </inlets>
    <outlets>
      <int32  name="chain" description="Chain step output"/>
      <frac32 name="out"  description="Unipolar output"/>
    </outlets>
    <displays/>
    <params>
      <frac32.u.map name="v1"/>
      <frac32.u.map name="v2"/>
      <frac32.u.map name="v3"/>
      <frac32.u.map name="v4"/>
    </params>
    <attribs/>
    <code.krate><![CDATA[
      switch( inlet_step )
        {
          case 0:  outlet_out = param_v1;  break;
          case 1:  outlet_out = param_v2;  break;
          case 2:  outlet_out = param_v3;  break;
          case 3:  outlet_out = param_v4;  break;
          default: outlet_out = 0;         break;
        }
      outlet_chain = inlet_step - 4;
    ]]></code.krate>
  </obj.normal>
