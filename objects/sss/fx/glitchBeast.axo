int32_t *Array;

static const uint32_t pLENGTHPOW = 10;
static const uint32_t pLENGTH = (1<<10);
static const uint32_t pLENGTHMASK = ((1<<10)-1);
int32_t *ARRAY;
int32_t Q[16];
uint32_t qpos;
