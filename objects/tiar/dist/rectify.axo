<objdefs appVersion="1.0.12">
   <obj.normal id="rectify" uuid="b317514a-7b72-4215-ae86-d5eecdbe10fc">
      <sDescription>Full wave rectifier.
Differentiated Anti aliasing.</sDescription>
      <author>Smashed Transistors</author>
      <license>BSD</license>
      <helpPatch>rectify.axh</helpPatch>
      <inlets>
         <frac32buffer.bipolar name="in"/>
      </inlets>
      <outlets>
         <frac32buffer name="out" description="out"/>
      </outlets>
      <displays/>
      <params/>
      <attribs/>
      <code.declaration><![CDATA[float x0 = 0, x1 = 0, y0 = 0, y1 = 0;]]></code.declaration>
      <code.srate><![CDATA[x1 = x0; y1 = y0;
x0 = arm::q_to_float(inlet_in, 27);
float _x0 = fabs(x0);
y0 = 0.5f * x0 * _x0;
float x1_x0 = x1 - x0;
if(fabs(x1_x0) > 0.001f){
  outlet_out = arm::float_to_q((y1 - y0) / (x1_x0), 27);
} else {
  outlet_out = arm::float_to_q(_x0, 27);
}]]></code.srate>
   </obj.normal>
</objdefs>