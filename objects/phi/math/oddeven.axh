         <displays/>
         <params/>
         <attribs/>
         <includes/>
         <code.krate><![CDATA[int odd;

if(((inlet_i+1) & inlet_i1) == 0)
    odd=1;
else
    odd=0;
    outlet_odd = odd;
    outlet_o = ((odd>0)?inlet_i:0);
    outlet_e = ((odd<1)?inlet_i:0);]]></code.krate>
