         <frac32buffer name="in" description="wave input"/>
      </inlets>
      <outlets/>
      <displays/>
      <params/>
      <attribs/>
      <code.declaration><![CDATA[static const uint32_t LENGTHPOW = (6);
static const uint32_t LENGTH = (1<<6);
static const uint32_t LENGTHMASK = ((1<<6)-1);
int32_t *array;
uint32_t writepos;]]></code.declaration>
      <code.init><![CDATA[static int32_t _array[attr_poly][1<<6]  __attribute__ ((section (".sdram")));
