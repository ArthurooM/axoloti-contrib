      <object id="patch/object" uuid="2693b524-791a-4063-be5d-33013c88c6e6">
         <sDescription>saw wave LFO, rising slope, frequency input</sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>lfo.axh</helpPatch>
         <inlets>
            <frac32 name="freq" description="frequency"/>
            <bool32.rising name="reset" description="reset phase"/>
         </inlets>
         <outlets>
            <frac32.positive name="wave" description="saw wave"/>
         </outlets>
         <displays/>
         <params/>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[int32_t Phase;
uint32_t r;]]></code.declaration>
         <code.init><![CDATA[   Phase = 0;
   r = 1;
]]></code.init>
         <code.krate><![CDATA[{
      if (inlet_reset && r) {
         Phase = 0;
         r = 0;
      } else {
         if (!inlet_reset) r = 1;
         Phase += inlet_freq>>2;
      }
      outlet_wave= (Phase>>4);
   }]]></code.krate>
      </object>
   </patchobj>
