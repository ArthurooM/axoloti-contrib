   <patchobj type="patch/object" uuid="89a6acf4-35be-4eb8-891e-3a043ad47766" name="wrp_1" x="1498" y="238">
      <params/>
      <attribs/>
      <object id="patch/object" uuid="89a6acf4-35be-4eb8-891e-3a043ad47766">
         <sDescription>wrap to 0..64 range</sDescription>
         <author>Robert Schirmer</author>
         <license>BSD</license>
         <helpPatch>math.axh</helpPatch>
         <inlets>
            <frac32 name="a" description="a"/>
         </inlets>
         <outlets>
            <frac32.positive name="b" description="quant(a)"/>
         </outlets>
         <displays/>
         <params/>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[int32_t v;
int32_t max = 1 << 27;
int32_t min = (-max);]]></code.declaration>
         <code.krate><![CDATA[v = inlet_a;
if (v > max) v = min + (v - max);
if (v < min) v = max - (min - v);
outlet_b = v;]]></code.krate>
      </object>
   </patchobj>
