         <patchobj type="patch/object" uuid="b46f7c38-d1a2-4bfc-89fc-a3805975a5d1" name="suffix_2" x="322" y="336">
            <params/>
            <attribs>
               <table attributeName="suffix1" table=""/>
               <table attributeName="suffix2" table=""/>
               <spinner attributeName="maxLength" value="16"/>
            </attribs>
            <object id="patch/object" uuid="b46f7c38-d1a2-4bfc-89fc-a3805975a5d1">
               <sDescription>appends a suffix to an input-string.
BEWARE:maximum length of input and suffix may not exceed 16 characters.</sDescription>
               <author>Robert Schirmer</author>
               <license>BSD</license>
               <inlets>
                  <charptr32 name="str1"/>
               </inlets>
               <outlets>
                  <charptr32 name="out1" description="string"/>
                  <charptr32 name="out2"/>
               </outlets>
               <displays/>
               <params/>
               <attribs>
                  <table name="suffix1"/>
                  <table name="suffix2"/>
                  <spinner name="maxLength" MinValue="1" MaxValue="32" DefaultValue="16"/>
               </attribs>
               <includes/>
               <code.declaration><![CDATA[char c1[attr_maxLength];
char c2[attr_maxLength];]]></code.declaration>
               <code.krate><![CDATA[strcpy(&c1[0],inlet_str1);
strcat(&c1[0],"attr_suffix1");
outlet_out1 = &c1[0];

strcpy(&c2[0],inlet_str1);
strcat(&c2[0],"attr_suffix2");
outlet_out2 = &c2[0];]]></code.krate>
            </object>
         </patchobj>
         <nets>
            <net>
               <source obj="prefix_1" outlet="out"/>
               <dest obj="suffix_1" inlet="str1"/>
            </net>
            <net>
               <source obj="suffix_1" outlet="out1"/>
               <dest obj="save_1" inlet="filename"/>
               <dest obj="load_1" inlet="filename"/>
            </net>
            <net>
               <source obj="suffix_1" outlet="out2"/>
               <dest obj="save_2" inlet="filename"/>
               <dest obj="load_2" inlet="filename"/>
            </net>
            <net>
               <source obj="bank" outlet="inlet"/>
               <dest obj="prefix_1" inlet="index"/>
            </net>
            <net>
               <source obj="load" outlet="inlet"/>
               <dest obj="load_1" inlet="trig"/>
               <dest obj="load_2" inlet="trig"/>
            </net>
            <net>
               <source obj="save" outlet="inlet"/>
               <dest obj="save_1" inlet="trig"/>
               <dest obj="save_2" inlet="trig"/>
