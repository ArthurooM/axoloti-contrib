      </attribs>
      <object id="patch/object" uuid="e0bfa327-cdca-4650-88d1-69557d8fc279">
         <inlets>
            <frac32 name="freq"/>
            <frac32 name="chunk size"/>
            <frac32 name="loop speed"/>
            <int32 name="nslice"/>
         </inlets>
         <outlets>
            <frac32buffer name="o"/>
         </outlets>
         <displays/>
         <params/>
         <attribs>
            <objref name="slicesbuffer"/>
            <objref name="slicesindex"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[int64_t pos;
uint32_t beg;
uint32_t end;
int64_t f0;
uint32_t nslice;]]></code.declaration>
         <code.init><![CDATA[pos = 0;
beg = 0;
end = 0;
f0 = 0;
nslice = 0xFFFF;]]></code.init>
         <code.krate><![CDATA[if(inlet_nslice < attr_slicesindex.array[0] && inlet_nslice != nslice) {
   nslice = inlet_nslice; 
   if(inlet_nslice == 0) {
      beg = 0;
      end = attr_slicesindex.array[1];
   } else {
      beg = (uint64_t)attr_slicesindex.array[inlet_nslice];
      end = attr_slicesindex.array[inlet_nslice+1];
   }
   pos = ((uint64_t)beg)<<32;
}

MTOFEXTENDED(inlet_freq + 80179668,f0);]]></code.krate>
         <code.srate><![CDATA[uint64_t inc = ((uint64_t)f0)<<4;
uint32_t o = ___SMMUL(attr_slicesbuffer.array[pos>>32]<<attr_slicesbuffer.GAIN,INT32_MAX-(((uint32_t)pos)>>1));
outlet_o = ___SMMLA(attr_slicesbuffer.array[(pos>>32)+1]<<attr_slicesbuffer.GAIN,(((uint32_t)pos)>>1),o);
pos += inc;
if(pos>>32 > end) pos = ((uint64_t)beg)<<32;]]></code.srate>
      </object>
   </patchobj>
